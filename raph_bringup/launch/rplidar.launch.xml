<launch version="0.1.1">

  <arg name="tf_frame_prefix"
    default=""
    description="The prefix added to each published TF frame id" />

  <node namespace="rplidar"
    name="rplidar_node"
    pkg="rplidar_ros"
    exec="rplidar_node">
    <param from="$(find-pkg-share raph_bringup)/config/rplidar.yaml" />
    <param name="frame_id" value="$(var tf_frame_prefix)laser_frame" />
  </node>

</launch>